<% provide(:title, 'Markets') %>
<div class="hireus_block">
  <div class="container_12">
    <section class="">
      <div class="wrapper">
        <p id="notice"><%= notice %></p>

        <p>
          <strong>Name:</strong>
          <%= @market.name %>
        </p>

        <p>
          <strong>Category:</strong>
          <%= @market.category %>
        </p>

        <p>
          <strong>Description:</strong>
          <%= @market.description %>
        </p>

        <p>
          <strong>Published date:</strong>
          <%= @market.published_date %>
        </p>

        <p>
          <strong>Arbitration date:</strong>
          <%= @market.arbitration_date %>
        </p>


        <%= link_to 'Contracts', market_contracts_path(@market) %> |
        <%= link_to 'Edit', edit_market_path(@market) %> 

      </div>
    </section>
  </div>
</div>

