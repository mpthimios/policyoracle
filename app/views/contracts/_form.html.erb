<%= form_for([@market, @contract]) do |f| %>
  
  <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :opening_price %><br>
    <%= f.text_field :opening_price %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
